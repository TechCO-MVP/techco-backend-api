{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "email": {
        "type": "string",
        "pattern": "^[^@\\s]+@[^@\\s]+\\.[^@\\s]+$"
      },
      "otp": {
        "type": ["string", "number"]
      },
      "session": {
        "type": "string"
      }
    },
    "required": [
      "email",
      "otp",
      "session"
    ]
  }